humid.server.host=localhost
humid.server.port=3000
humid.server.prefix=/api
humid.app.version=v1
humid.server.doc-prefix=swagger
node_env=development
file.img-location=resources/images

# File serving CORS configuration
file.enable-cors=false
file.cors-allowed-origins=http://localhost:4200

# Crawling concurrency configuration
crawl.chapter.concurrency=2
crawl.image.concurrency=3
crawl.queue.log-level=info

# PostgreSQL Database Configuration
db.host=localhost
db.port=5432
db.username=clouduser
db.password=cloudpass
db.database=humid_db

nettruyen-domain=https://nettruyenrr.com
